algoritmo "algoritmo53"
// 
var
numero: real  
mult : real
contador:inteiro



inicio
 mult<-1

   
   enquanto contador <> 3  faca
   
      escreva ("Digite numero: ")
      leia(numero)
     
      mult <- mult * numero
     
      contador <- contador + 1
     
   fimenquanto
   escreva("A multiplicação dos numero digitados é: ", mult)



fimalgoritmo