Algoritmo "ATIVIDADE5"

Var
   Nome : caracter
   Sobrenome : caracter
   NomeCompleto: caracter

Inicio

   escreva("Digite o nome: ")
   leia(nome)
   escreva("Digite o sobrenome: ")
   leia(sobrenome)
   NomeCompleto <- nome +" "+ sobrenome
   escreva("o nome completo é " , NomeCompleto)
Fimalgoritmo