algoritmo "Nome do Programa"
// fazer um quadrado com asteriscos
var
linha,coluna:inteiro
inicio
linha<-8
enquanto (linha>=1) faca
   coluna<-1
   enquanto (coluna<=8) faca
    
        escreva("* ")
        escreva (" *")
     
    coluna<-coluna+1
   fimenquanto
   escreval()
   linha<-linha-1
fimenquanto




fimalgoritmo