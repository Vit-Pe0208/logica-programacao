Algoritmo "Atividade6"

Var
   num1,num2,num3,soma: real
   
Inicio
   escreva("Digite o primeiro número: ")
   leia(num1)
   escreva("Digite o segundo número: ")
   leia(num2)
   escreva("Digite o terceiro número: ")
   leia(num3)
   soma<-num1+num2+num3
   escreva("O valor da soma é:",soma)
   
Fimalgoritmo