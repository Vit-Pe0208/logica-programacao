Algoritmo "algoritmo21"

Var
 num1,num2,divisao1,divisao2:inteiro


Inicio
escreva("digite o numero 1 número: ")
leia(num1)
escreva("digite o numero2: ")
leia(num2)

se(num1>num2)entao
   divisao1<-num1 DIV num2
   ESCREVA("A DIVISÃO É: ",divisao1)
fimSe
 se (num1<num2)entao
    divisao2<-num2 DIV num1
    ESCREVA("A DIVISÃO É: ",divisao2)
fimSe
    

Fimalgoritmo